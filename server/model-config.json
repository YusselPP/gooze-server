{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../node_modules/loopback-ds-timestamp-mixin",
      "../node_modules/loopback-softdelete-mixin4",
      "../common/mixins",
      "./mixins"
    ]
  },
  "User": {
    "dataSource": "db",
    "public": false
  },
  "AccessToken": {
    "dataSource": "mongoDs",
    "public": false
  },
  "ACL": {
    "dataSource": "db",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "db",
    "public": false,
    "options": {
      "strictObjectIDCoercion": true
    }
  },
  "Role": {
    "dataSource": "db",
    "public": false
  },
  "GoozeUser": {
    "dataSource": "mongoDs",
    "public": true
  },
  "GoozeAccessToken": {
    "dataSource": "mongoDs",
    "public": false
  },
  "DateRequest": {
    "dataSource": "mongoDs",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "find": true
        }
      }
    }
  },
  "ChatMessage": {
    "dataSource": "mongoDs",
    "public": false
  },
  "Chat": {
    "dataSource": "mongoDs",
    "public": false
  },
  "container": {
    "dataSource": "storage",
    "public": true
  }
}
