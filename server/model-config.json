{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../node_modules/loopback-ds-timestamp-mixin",
      "../node_modules/loopback-softdelete-mixin4",
      "../common/mixins",
      "./mixins"
    ]
  },
  "User": {
    "dataSource": "db",
    "public": false
  },
  "AccessToken": {
    "dataSource": "mongoDs",
    "public": false
  },
  "ACL": {
    "dataSource": "mongoDs",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "mongoDs",
    "public": false,
    "options": {
      "strictObjectIDCoercion": true
    }
  },
  "Role": {
    "dataSource": "mongoDs",
    "public": false
  },
  "AppConfig": {
    "dataSource": "mongoDs",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "findByName": true
        }
      }
    }
  },
  "GoozeUser": {
    "dataSource": "mongoDs",
    "public": true
  },
  "GoozeAccessToken": {
    "dataSource": "mongoDs",
    "public": false
  },
  "DateRequest": {
    "dataSource": "mongoDs",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "find": true,
          "startDate": true,
          "endDate": true,
          "prototype.patchAttributes": true
        }
      }
    }
  },
  "ChatMessage": {
    "dataSource": "mongoDs",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "updateAll": true,
          "count": true
        }
      }
    }
  },
  "Chat": {
    "dataSource": "mongoDs",
    "public": false
  },
  "GZEDate": {
    "dataSource": "mongoDs",
    "public": true
  },
  "GZERateComment": {
    "dataSource": "mongoDs",
    "public": true
  },
  "UserComments": {
    "dataSource": "mongoDs",
    "public": false
  },
  "Payment": {
    "dataSource": "mongoDs",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "createCustomer": true,
          "findCustomer": true,
          "findPaymentMethods": true,
          "createPaymentMethod": true,
          "deletePaymentMethod": true,
          "clientToken": true,
          "createCharge": true
        }
      }
    }
  },
  "DeviceToken": {
    "dataSource": "mongoDs",
    "public": true
  },
  "container": {
    "dataSource": "storage",
    "public": true
  }
}
