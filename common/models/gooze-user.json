{
  "name": "GoozeUser",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": {
      "validateUpsert": true,
      "required": true
    }
  },
  "properties": {
    "username": {
      "type": "string",
      "required": true
    },
    "registerCode": {
      "type": "string"
    },
    "invitedBy": {
      "type": "string"
    },
    "birthday": {
      "type": "date"
    },
    "gender": {
      "type": "string",
      "default": null
    },
    "weight": {
      "type": "number"
    },
    "height": {
      "type": "number"
    },
    "languages": {
      "type": [
        "string"
      ]
    },
    "interestedIn": {
      "type": [
        "string"
      ]
    },
    "profilePic": {
      "type": {
        "container": "string",
        "url": "string",
        "name": "string",
        "blocked": "boolean"
      }
    },
    "searchPic": {
      "type": {
        "container": "string",
        "url": "string",
        "name": "string",
        "blocked": "boolean"
      }
    },
    "photos": {
      "type": "array"
    },
    "origin": {
      "type": "string"
    },
    "phrase": {
      "type": "string"
    },
    "mode": {
      "type": "string",
      "required": true,
      "default": "client"
    },
    "status": {
      "type": "string",
      "required": true,
      "default": "unavailable"
    },
    "loggedIn": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "currentLocation": {
      "type": "GeoPoint"
    },
    "activeUntil": {
      "type": "date"
    }
  },
  "validations": [],
  "relations": {},
  "acls": [
    {
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "findByLocation"
    },
    {
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "publicProfile"
    }
  ],
  "methods": {}
}
