{
  "name": "GoozeUser",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": {
      "validateUpsert": true,
      "required": true
    }
  },
  "properties": {
    "username": {
      "type": "string",
      "required": true
    },
    "registerCode": {
      "type": "string"
    },
    "invitedBy": {
      "type": "string"
    },
    "birthday": {
      "type": "date"
    },
    "gender": {
      "type": "string",
      "default": null
    },
    "weight": {
      "type": "number"
    },
    "height": {
      "type": "number"
    },
    "languages": {
      "type": [
        "string"
      ]
    },
    "interestedIn": {
      "type": [
        "string"
      ]
    },
    "photos": {
      "type": "array"
    },
    "origin": {
      "type": "string"
    },
    "phrase": {
      "type": "string"
    },
    "mode": {
      "type": "string",
      "required": true,
      "default": "client"
    },
    "status": {
      "type": "string",
      "required": true,
      "default": "unavailable"
    },
    "loggedIn": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "currentLocation": {
      "type": "GeoPoint"
    }
  },
  "validations": [],
  "relations": {},
  "acls": [],
  "methods": {}
}
