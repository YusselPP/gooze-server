{
  "name": "UserTransaction",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": {
      "required": true
    }
  },
  "properties": {
    "amount": {
      "type": "string",
      "required": true
    },
    "status": {
      "type": "string",
      "required": true
    },
    "paymentMethod": {
      "type": "string",
      "required": true
    },
    "goozeStatus": {
      "type": "string",
      "required": true,
      "default": "pending"
    }
  },
  "validations": [],
  "relations": {
    "dateRequest": {
      "type": "belongsTo",
      "model": "DateRequest",
      "foreignKey": "dateRequestId"
    },
    "fromUser": {
      "type": "belongsTo",
      "model": "GoozeUser",
      "foreignKey": "fromUserId"
    },
    "toUser": {
      "type": "belongsTo",
      "model": "GoozeUser",
      "foreignKey": "toUserId"
    }
  },
  "acls": [
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "find"
    },
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "accessType": "EXECUTE",
      "property": "paymentReport"
    }
  ],
  "scopes": {},
  "methods": {}
}
